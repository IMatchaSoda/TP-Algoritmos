using System;

namespace TPAlgoritmos_Constructora
{
	/// <summary>
	/// Description of Empresa_Constructora.
	/// </summary>
	public class Jefe_Obrero:Obrero
	{
		private double bono;
		Grupo_Obrero grupo;
		public Jefe_Obrero(double bono,Grupo_Obrero grupo):base(string nom,string apellido,string cargo,double sueldo,int legajo,int dni)
		{
			this.bono=bono;
			this.grupo=grupo;
		}
		
	}
}
